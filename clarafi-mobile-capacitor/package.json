    "@ionic/react": "^7.8.2",
    "@ionic/react-router": "^7.8.2",
    "@tanstack/react-query": "^5.17.9",
    "ionicons": "^7.4.0",
    "lucide-react": "^0.263.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.3"
